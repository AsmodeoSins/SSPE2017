<Controls:MetroWindow x:Class="ControlPenales.PrincipalView"
                      Name="MainMenu"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
                      xmlns:dic="clr-namespace:Resources.ControlPenales.Login;assembly=Resources"
                      xmlns:toastinetWpf="clr-namespace:ToastinetWPF;assembly=ToastinetWPF"
                      xmlns:Dialog="clr-namespace:MahApps.Metro.Controls.Dialogs;assembly=MahApps.Metro"
                      xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                      Title="{Binding Titulo}"
                      WindowStartupLocation="Manual"
                      xmlns:local="clr-namespace:ControlPenales"
                      xmlns:cmd="http://www.galasoft.ch/mvvmlight"
                      xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                      GlowBrush="{DynamicResource AccentColorBrush}"
                      NonActiveGlowBrush="Red"
                      BorderThickness="1"
                      BorderBrush="#3FAFDF"
                      Icon="/ControlPenales;component/Imagen/favicon.ico"
                      WindowState="Maximized">
    <!--VALIDAMOS AL CERRAR APLICACION-->
    <i:Interaction.Triggers>

        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding WindowLoad}"
                                   CommandParameter="{Binding ElementName=MainMenu}" />
        </i:EventTrigger>
        <i:EventTrigger EventName="PreviewMouseMove">
            <i:InvokeCommandAction Command="{Binding MouseMove}"
                                   CommandParameter="{Binding ElementName=MainMenu}" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Window.Resources>
        <local:MultiValueConverter x:Key="MultiParamConverter"></local:MultiValueConverter>
        <!--MENSAJE DE CONFIRMACION-->
        <Dialog:CustomDialog x:Key="ConfirmacionDialog"
                             Title="Confirmación"
                             x:Name="ConfirmacionDialog">
            <TextBlock Height="30"
                       Text="Se ha eliminado correctamente."
                       FontSize="15"
                       FontWeight="Bold" />
        </Dialog:CustomDialog>
        <Dialog:CustomDialog x:Key="ErrorDialog"
                             Title="Confirmación"
                             x:Name="ErrorDialog">
            <TextBlock Height="30"
                       Text="Error al eliminar."
                       FontSize="15"
                       FontWeight="Bold" />
        </Dialog:CustomDialog>
        <!--MENSAJE DE CONFIRMACION-->
        <Dialog:CustomDialog x:Key="ErrorIngresoDialog"
                             Title="Confirmación"
                             x:Name="ErrorIngresoDialog">
            <TextBlock Height="30"
                       Text="Error"
                       FontSize="15"
                       FontWeight="Bold" />
        </Dialog:CustomDialog>
        <!--NOTIFICACION DIALOG-->
        <Dialog:CustomDialog x:Key="NotificacionDialog"
                             Title="{Binding DialogTitulo}"
                             x:Name="NotificacionDialog">
            <TextBlock Height="30"
                       Text="{Binding DialogMensaje}"
                       FontSize="15"
                       FontWeight="Bold" />
        </Dialog:CustomDialog>
        <!--CONFIRMACION DIALOG-->
        <Dialog:CustomDialog x:Key="ConfirmacionDialogo"
                             Title="{Binding ConfirmacionTitulo}"
                             x:Name="ConfirmacionDialogo">
            <TextBlock Height="30"
                       Text="{Binding ConfirmacionMensaje}"
                       FontSize="15"
                       FontWeight="Bold" />
        </Dialog:CustomDialog>
        <!--BANDEJA ENTRADA-->
        <Dialog:CustomDialog x:Key="ErrorValidacionDialog"
                             Title="Confirmación"
                             x:Name="ErrorValidacionDialog">
            <TextBlock Height="30"
                       Text="Error"
                       FontSize="15"
                       FontWeight="Bold" />
        </Dialog:CustomDialog>
        <Canvas x:Key="appbar_people_profile_black"
                Width="76"
                Height="76"
                Clip="F1 M 0,0L 76,0L 76,76L 0,76L 0,0">
