    <Grid Background="#90000000">
        <Grid Background="White"
              Name="grid_Main"
              Visibility="{Binding LockMenu,Source={StaticResource PopUpsSources}}">
            <Menu IsEnabled="{Binding EnabledMenu,Source={StaticResource PopUpsSources}}"
                  Grid.Column="1"
                  Grid.Row="0"
                  VerticalAlignment="Top"
                  HorizontalAlignment="Left"
                  Width="{Binding ElementName=grid_Main,Path=ActualWidth}">
                <MenuItem Header="Archivo">
                    <!--<MenuItem Header="Establecer Conexion..."></MenuItem>
                    <Separator></Separator>
                    <MenuItem Header="Configurar Impresor..."></MenuItem>
                    <Separator></Separator>-->
                    <MenuItem Header="Salir"
                              Command="{Binding OnAppClose}"
                              CommandParameter="{Binding ElementName=MainMenu}"></MenuItem>
                </MenuItem>
                <MenuItem Header="Proceso">
                    <MenuItem Header="Registro">
                        <MenuItem Header="Registro"
                                  Command="{Binding ChangePageCommand}"
                                  Visibility="{Binding MRegistroV}" IsEnabled="{Binding MRegistro}">
                            <MenuItem.CommandParameter>
                                <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                    <Binding Source="ControlPenales.RegistroIngresoViewModel"></Binding>
                                    <Binding Source="ControlPenales.RegistroIngresoView"></Binding>
                                </MultiBinding>
                            </MenuItem.CommandParameter>
                        </MenuItem>
                        <Separator Visibility="{Binding MDecomisoV}"></Separator>
                        <MenuItem Header="Decomisos"
                                  Command="{Binding ChangePageCommand}"
                                  Visibility="{Binding MDecomisoV}" IsEnabled="{Binding MDecomiso}">
                            <MenuItem.CommandParameter>
                                <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                    <Binding Source="ControlPenales.RegistroDecomisoViewModel"></Binding>
                                    <Binding Source="ControlPenales.RegistroDecomisoView"></Binding>
                                </MultiBinding>
                            </MenuItem.CommandParameter>
                        </MenuItem>
                        <Separator Visibility="{Binding MDecomisoV}"></Separator>
                        <MenuItem Header="Correspondencia"
                                  Command="{Binding ChangePageCommand}"
                                  Visibility="{Binding MDecomisoV}" IsEnabled="{Binding MCorrespondencia}">
                            <MenuItem.CommandParameter>
                                <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                    <Binding Source="ControlPenales.CorrespondenciasViewModel"></Binding>
                                    <Binding Source="ControlPenales.RegistroCorrespondenciaView"></Binding>
                                </MultiBinding>
                            </MenuItem.CommandParameter>
                        </MenuItem>
                    </MenuItem>
                    <Separator Visibility="{Binding MSeguimientoMV}"></Separator>
                    <MenuItem Header="Seguimiento"
                              Visibility="{Binding MSeguimientoMV}">
                        <MenuItem Header="Juridico"
                                  Visibility="{Binding MJuridicoMV}">
                            <MenuItem Header="Causas Penales"
                                      Command="{Binding ChangePageCommand}"
                                      Visibility="{Binding MCausaPenalV}" IsEnabled="{Binding MCausaPenal}">
                                <MenuItem.CommandParameter>
                                    <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                        <Binding Source="ControlPenales.CausaPenalViewModel"></Binding>
                                        <Binding Source="ControlPenales.CausaPenalView"></Binding>
                                    </MultiBinding>
                                </MenuItem.CommandParameter>
                            </MenuItem>
                            <MenuItem Header="Identificacion"
                                      Command="{Binding ChangePageCommand}"
                                      Visibility="{Binding MIdentificacionV}"  IsEnabled="{Binding MIdentificacion}">
                                <MenuItem.CommandParameter>
                                    <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                        <Binding Source="ControlPenales.JuridicoIdentificacionViewModel"></Binding>
                                        <Binding Source="ControlPenales.RegistroIngresoView"></Binding>
                                    </MultiBinding>
                                </MenuItem.CommandParameter>
                            </MenuItem>
                            <MenuItem Header="Estatus Administrativo"
                                      Command="{Binding ChangePageCommand}"
                                      Visibility="{Binding MEstatusAdministrativoV}"  IsEnabled="{Binding MEstatusAdministrativo}">
                                <MenuItem.CommandParameter>
                                    <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                        <Binding Source="ControlPenales.EstatusAdministrativoViewModel"></Binding>
                                        <Binding Source="ControlPenales.EstatusAdministrativoView"></Binding>
                                    </MultiBinding>
                                </MenuItem.CommandParameter>
                            </MenuItem>
                            <MenuItem Header="Estudios de Beneficio">
                                <MenuItem Header="Creación de Listas"
                                          Command="{Binding ChangePageCommand}"
                                          Visibility="{Binding MRealizacionListasV}" IsEnabled="{Binding MCreacionListas}">
                                    <MenuItem.CommandParameter>
                                        <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                            <Binding Source="ControlPenales.CreacionListasExamenPViewModel"></Binding>
                                            <Binding Source="ControlPenales.CreacionListasExamenPView"></Binding>
                                        </MultiBinding>
                                    </MenuItem.CommandParameter>
                                </MenuItem>
                            </MenuItem>
                            <!--<MenuItem Header="Notificaciones">
                                <MenuItem Header="Audiciones" IsEnabled="False"></MenuItem>
                                <MenuItem Header="Resolutivos" IsEnabled="False"></MenuItem>
                            </MenuItem>-->
                        </MenuItem>
                        <Separator Visibility="{Binding VAdministrativoMV}"></Separator>
                        <MenuItem Header="Administrativo"
                                  Visibility="{Binding VAdministrativoMV}">
                            <MenuItem Header="Pertenencias"
                                      Command="{Binding ChangePageCommand}"
                                      Visibility="{Binding MPertenenciasV}" IsEnabled="{Binding MPertenencias}">
                                <MenuItem.CommandParameter>
                                    <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                        <Binding Source="ControlPenales.RegistroPertenenciasViewModel"></Binding>
                                        <Binding Source="ControlPenales.RegistroPertenenciasView"></Binding>
                                    </MultiBinding>
                                </MenuItem.CommandParameter>
                            </MenuItem>
                            <MenuItem Header="Ubicación de Estancias"
                                      Command="{Binding ChangePageCommand}"
                                      Visibility="{Binding MUbicacionEstanciaV}"  IsEnabled="{Binding MUbicacionEstancia}">
                                <MenuItem.CommandParameter>
                                    <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                        <Binding Source="ControlPenales.UbicacionEstanciaViewModel"></Binding>
                                        <Binding Source="ControlPenales.UbicacionEstanciaView"></Binding>
                                    </MultiBinding>
                                </MenuItem.CommandParameter>
                            </MenuItem>
                            <MenuItem Header="Incidentes"
                                      Command="{Binding ChangePageCommand}"
                                      Visibility="{Binding MIncidentesV}"  IsEnabled="{Binding MIncidentes}">
                                <MenuItem.CommandParameter>
                                    <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                        <Binding Source="ControlPenales.SancionesDisciplinariasViewModel"></Binding>
                                        <Binding Source="ControlPenales.SancionesDisciplinariasView"></Binding>
                                    </MultiBinding>
                                </MenuItem.CommandParameter>
                            </MenuItem>
                            <!--<MenuItem Header="Recursos Humanos" CommandParameter="recursos_humanos"></MenuItem>-->
                        </MenuItem>
                        <Separator Visibility="{Binding MTecnicoMV}"></Separator>
                        <MenuItem Header="Técnico"
                                  Visibility="{Binding MTecnicoMV}">
                            <MenuItem Header="Estudios">
                                <MenuItem Header="_Estudio Multidiciplinario Inicial"
                                          Command="{Binding ChangePageCommand}"
                                          Visibility="{Binding MEMIV}"  IsEnabled="{Binding MEMI}">
                                    <MenuItem.CommandParameter>
                                        <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                            <Binding Source="ControlPenales.EntrevistaMultidiciplinariaViewModel"></Binding>
                                            <Binding Source="ControlPenales.EntrevistaMultidiciplinariaView"></Binding>
                                        </MultiBinding>
                                    </MenuItem.CommandParameter>
                                </MenuItem>
                                <MenuItem Header="Entrevista Inicial de Trabajo Social"
                                          Command="{Binding ChangePageCommand}"
                                          Visibility="{Binding MEntrevistaInicialTrabajoSocialV}"  IsEnabled="{Binding MEntrevistaInicialTS}">
                                    <MenuItem.CommandParameter>
                                        <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                            <Binding Source="ControlPenales.EntrevistaInicialViewModel"></Binding>
                                            <Binding Source="ControlPenales.EntrevistaInicialView"></Binding>
                                        </MultiBinding>
                                    </MenuItem.CommandParameter>
                                </MenuItem>
                                <!--<MenuItem Header="Entrevista Inicial Trabajo Social..." Command="{Binding ChangePageCommand}" Visibility="{Binding MEntrevistaInicialTrabajoSocialV}">
                                    <MenuItem.CommandParameter>
                                        <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                            <Binding Source="ControlPenales.EntrevistaInicialViewModel"></Binding>
                                            <Binding Source="ControlPenales.EntrevistaInicialView"></Binding>
                                        </MultiBinding>
                                    </MenuItem.CommandParameter>
                                </MenuItem>-->
                                <MenuItem Header="Estudio Socio Economico"
                                          Command="{Binding ChangePageCommand}"
                                          Visibility="{Binding MEstudioSocioeconomicoV}"  IsEnabled="{Binding MEstudioSocioEconomico}">
                                    <MenuItem.CommandParameter>
                                        <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                            <Binding Source="ControlPenales.EstudioSocioEconomicoViewModel"></Binding>
                                            <Binding Source="ControlPenales.EstudioSocioEconomicoView"></Binding>
                                        </MultiBinding>
                                    </MenuItem.CommandParameter>
                                </MenuItem>
                                <MenuItem Header="Estudios de Personalidad">
                                    <MenuItem Header="Programación"
                                              Command="{Binding ChangePageCommand}"
                                              Visibility="{Binding MEstudioPersonalidadV}"  IsEnabled="{Binding MProgramacionEstudios}">
                                        <MenuItem.CommandParameter>
                                            <MultiBinding Converter="{StaticResource MultiParamConverter}">
                                                <Binding Source="ControlPenales.EstudioPersonalidadViewModel"></Binding>
                                                <Binding Source="ControlPenales.EstudioPersonalidadView"></Binding>
                                                <Binding Source="1"></Binding>
