                                    <Button Grid.Column="0" Grid.Row="1" VerticalAlignment="Top" Width="150" Style="{DynamicResource AccentedSquareButtonStyle}" Margin="10"
                CommandParameter="agregar_odonto_inicial" Command="{Binding OnClick}" Content="Agregar" IsEnabled="{ Binding IsEnabledBotonesProcesoOdontogramas, UpdateSourceTrigger=PropertyChanged}"></Button>
                                    <Button Grid.Column="1" Grid.Row="1" VerticalAlignment="Top" Width="150" Style="{DynamicResource AccentedSquareButtonStyle}" Margin="10"
                CommandParameter="limpiar_odonto_inicial" Command="{Binding OnClick}" Content="Limpiar Selección" IsEnabled="{ Binding IsEnabledBotonesProcesoOdontogramas, UpdateSourceTrigger=PropertyChanged}"></Button>
                                <!--</StackPanel>-->

                            </Grid>
                        </AdornerDecorator>
                    </GroupBox>
                    <GroupBox Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="5" Grid.RowSpan="5" Header="Registro">
                        <DataGrid Grid.Row="0" Name="dgDientesInicial" Grid.Column="2" Grid.RowSpan="5" Height="200" Background="WhiteSmoke" AutoGenerateColumns="False"  
                              CanUserAddRows="False" ItemsSource="{Binding LstOdontogramaInicial}" SelectedItem="{Binding SelectedDiente1}" IsReadOnly="True" Style="{StaticResource MetroDataGrid}" SelectionMode="Single">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="MouseDoubleClick" >
                                    <i:InvokeCommandAction CommandParameter="{Binding ElementName=dgDientesInicial,Path=SelectedItem}" Command="{Binding DoubleClickGridCommand}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                            <DataGrid.Columns>
                                <DataGridTextColumn Binding="{Binding Converter={StaticResource TratamientoEnfermedadDentalConverter}}" Header="Tipo"></DataGridTextColumn>
                                <!--<DataGridTextColumn Binding="{Binding Converter={StaticResource TratamientoEnfermedadDescripcionConverter}}" Header="Nombre"></DataGridTextColumn>-->
                                <DataGridTextColumn Header="Odontograma">
                                    <DataGridTextColumn.Binding>
                                        <MultiBinding Converter="{ StaticResource TratamientoEnfermedadDescripcionConverter}">
                                            <Binding Path="ID_TIPO_ODO" FallbackValue="-1"></Binding>
                                            <Binding Path="ID_POSICION" FallbackValue="-1"></Binding>
                                            <Binding Path="ID_ENFERMEDAD" FallbackValue="-1"></Binding>
                                            <Binding Path="ID_TRATA" FallbackValue="-1"></Binding>
                                        </MultiBinding>
                                    </DataGridTextColumn.Binding>
                                </DataGridTextColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                    </GroupBox>
                </Grid>
            </GroupBox>
        </Grid>
    </AdornerDecorator>
</UserControl>
