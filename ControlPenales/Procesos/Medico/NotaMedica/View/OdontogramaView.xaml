                                                  SelectedValue="{ Binding IdSelectedEnfermedadDental, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True, Mode=TwoWay}" 
                                                  ItemsSource="{ Binding LstEnfermedadesDentales}" SelectedValuePath="ID_ENFERMEDAD" DisplayMemberPath="NOMBRE"></ComboBox>
                                        </Grid>
                                    </AdornerDecorator>
                                </GroupBox>
