                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Controls:ToggleSwitch VerticalAlignment="Center"
                                       IsChecked="{Binding SelectedAplicaTratamientoDental }"
                                       Grid.Column="0" Grid.Row="0"  Grid.ColumnSpan="2"
                                       OnLabel="Tratamiento SI"
                                       OffLabel="Tratamiento NO" />
                                            <Label Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" Content="Tratamiento"></Label>
